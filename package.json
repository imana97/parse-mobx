{
  "name": "parse-mobx",
  "version": "4.1.0",
  "description": "A wrapper for ParseJS SDK to make Parse Objects observable in Mobx",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "gen-doc": "npx typedoc src/index.ts",
    "serve-doc": "npx http-server docs/",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "bump:minor": "npm version minor",
    "bump:major": "npm version major",
    "tag:create": "git tag v$(node -p \"require('./package.json').version\")"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imana97/parse-mobx.git"
  },
  "keywords": [
    "parse",
    "parse",
    "platform",
    "parse",
    "js",
    "mobx",
    "react-mobx",
    "mobx-react",
    "parse",
    "mobx"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imana97/parse-mobx/issues"
  },
  "homepage": "https://github.com/imana97/parse-mobx#readme",
  "peerDependencies": {
    "mobx": "^6.13.0",
    "parse": "^6.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/parse": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.27.4",
    "typescript": "^5.7.2"
  },
  "files": [
    "lib/**/*"
  ]
}
