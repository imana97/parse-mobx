{
  "name": "parse-mobx",
  "version": "1.1.11",
  "description": "A wrapper for ParseJS SDK to make Parse Objects observable in Mobx",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "gen-doc": "npx typedoc src/index.ts",
    "serve-doc": "npx http-server docs/",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imana97/parse-mobx.git"
  },
  "keywords": [
    "parse",
    "parse",
    "platform",
    "parse",
    "js",
    "mobx",
    "react-mobx",
    "mobx-react",
    "parse",
    "mobx"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imana97/parse-mobx/issues"
  },
  "homepage": "https://github.com/imana97/parse-mobx#readme",
  "devDependencies": {
    "@types/jest": "29.2.4",
    "@types/parse": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.47.1",
    "eslint": "8.30.0",
    "eslint-plugin-jest": "27.1.7",
    "jest": "29.3.1",
    "prettier": "2.8.1",
    "ts-jest": "29.0.3",
    "typedoc": "^0.23.25",
    "typescript": "4.9.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "mobx": "^6.12.3"
  }
}
